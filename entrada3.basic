DIM VEC[11] AS INTEGER 
LLENAVECTOR()
PARIMPAR()
END

SUB LLENAVECTOR()
	DIM I AS INTEGER
	FOR I=0 TO 11 
		VEC[I]=I 
	NEXT I
END SUB

SUB PARIMPAR()
	DIM I AS INTEGER
	FOR I=0 TO 11 
		IF(VEC[I] MOD 2 = 0) THEN
			PRINT "El valor "; I ; "es PAR"
		ELSE
			PRINT "El valor "; I ; "es INPAR"
		ENDIF
	NEXT I
END SUB
